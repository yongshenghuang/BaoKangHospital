<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hr.vo">
	<class name="User" table="user">
		<!--  帐号ID -->
		<id name="id" column="id">
			<!-- 指定主键生成器策略 -->
			<generator class="native" />
		</id>
		<!-- 映射普通属性 -->
		<!--   帐号-->
		<property name="email" type="string"></property>
		<!--  密码-->
		<property name="image_uuid" type="string"></property>
		<property name="password" type="string"></property>
		<property name="ismember" type="int"></property>
		<property name="register_time"  type="java.sql.Timestamp"></property>
		<property name="type" type="int"></property>
         <property name="online" type="int"></property>
         <property name="truename" type="string"></property>
	 <property name="tel" type="string"></property>
    
    </class>
</hibernate-mapping>
